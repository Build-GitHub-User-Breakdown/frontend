{
    "version": 2,
    "name": "git-status",
    "builds": [{ "src": "git-status/package.json", "use": "@now/static-build", "config": {"distDir": "build"} }],
    "routes": [
        { "src": "/(.*)", "dest": "git-status/$1" }
    ],
    "build": {
        "env": {
          "REACT_APP_GITHUB_CLIENT_ID": "@react_app_github_client_id",
          "REACT_APP_GITHUB_CLIENT_SECRET": "@react_app_github_client_secret"
        }
      }
}